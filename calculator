print("The Love Calculator is calculating your score...")
name1 = input()
name2 = input()
join = name1 + name2
lower = join.lower()
t = lower.count("t")
r = lower.count("r")
u = lower.count("u")
e = lower.count("e")
first = t + r + u + e
l = lower.count("l")
o = lower.count("o")
v = lower.count("v")
e = lower.count("e")
second = l + o + v + e
score = str(first) + str(second)
final = int(score)
if final <= 10 or final >= 90:
  print(f"Your score is {final}, you go together like coke and mentos.")
elif final>= 40 and final<=50:
  print(f"Your score is {final}, you are alright together.")
else:
  print(f"Your score is {final}.")
